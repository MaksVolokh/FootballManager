@using FootballManagerDAL.Entities
@using FootballManagerAPI.Hubs
@model IEnumerable<Chat>

@{
    var userId = User.GetUserId;
}

<div class="chat-body">
    @foreach (var chat in Model)
    {
        <a class="room-button" asp-controller="Chat" asp-action="ChatAsync" asp-route-id="@chat.Id">
        </a>
    }
</div>
